package br.com.dev2me;

import javax.swing.jOptionPane;

public class Aula1github {

  public static void main(String[] args) {
  int n1 = Intereger.valueOf(JOption.showInputDialog("Digite seu numero 1"));
  int n2 = Intereger.valueOf(JOption.showInputDialog("Digite seu numero 2"));
  
  int soma = 0;
  soma = n1 + n2;
  
  System.out.println(soma);
  }
  }
  
